    <% #caching 10 versions of the home page
    cache_version ||= params[:cache_version]
    version=cache_version||Random.new.rand(10)
    begin
      fragment_file = Rails.cache.send(:read_entry,"views/#{I18n.locale} homepage_#{version}",{})
      if cache_version || (fragment_file && Time.now - 30.minutes > Time.at(fragment_file.created_at)) then
        ActionController::Base.new.expire_fragment "#{I18n.locale} homepage_#{version}"
      end
    rescue
      Rails.logger.warn $!.message
    end
    cache "#{I18n.locale} homepage_#{version}" do %>

      <%
      posts = BlogPost.where(:published => true).order("published_at DESC").limit(2)
      @blog_data = []
      monthnames = [nil] + %w(Jan Feb Mar Apr May Jun Jul Aug Sep Oct Nov Dec)

      posts.each do |blog|
        blog_line = {}
        date = blog.published_at
        blog_line["day"] = date.day
        blog_line["month"] = I18n.t('abbr_month_names', scope: [:date])[date.month]
        if I18n.locale == :en then
          blog_line["ext"] = blog_line["day"].ordinalize.gsub(/[0-9]/,'')
        end
        blog_line["title"] = blog.title
        #stripped to 220 chars
        #blog_line["txt"] = strip_tags(blog["post_content"]).gsub(/[\r\n\t]/,"").gsub(/\[.*\]/,"")[0..230]
        blog_line["txt"] = (blog.wiki_html.gsub(/\[[^\]]*\]/,"").gsub(/[\r\n\t]/,"").gsub(/<[^\>]*>/,""))[0..230]
        blog_line["url"] = blog.fullpermalink(:locale)
        # blog_line["url"] = "/blog/#{category}/#{date.year}/#{(date.day/10).to_i.to_s}#{date.day.to_s}/"+blog["post_name"]
        @blog_data << blog_line
      end


      begin
        interval = -30
        requested = 8
        while (@pictures.nil? || @pictures.count < requested) && (interval+=60) < 200 do
          @pictures = Picture.unscoped.includes(:picture_album_pictures => :dive).where('pictures.great_pic = 1 AND ROUND(width/height,1)=1.3 AND dives.privacy=0 AND pictures.updated_at > DATE_SUB(NOW(), interval ? day)', interval).order('rand()').limit(requested).to_ary
        end
        @pictures = nil if @pictures.blank?
      rescue
        @pictures = nil
      end%>

